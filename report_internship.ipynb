    "# Function to read FITS files\n",
    "def read_fits(file):\n",
    "    with fits.open(fits_file) as hdul:\n",
    "        header = hdul[0].header\n",
    "        data = hdul[0].data\n",
    "\n",
    "    crval1 = header['CRVAL1']\n",
    "    crpix1 = header['CRPIX1']\n",
    "    cdelt1 = header['CDELT1']\n",
    "    npix   = header['NAXIS1']\n",
    "\n",
    "    # Wavelenght (in Å)\n",
    "    x = crval1 + (np.arange(npix) - (crpix1 - 1)) * cdelt1\n",
    "    return x, data\n",
    "    "
